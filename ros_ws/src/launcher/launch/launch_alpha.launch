<launch>

	<arg name="simFile" default="SimulationKinetic" doc="Name of the simulation file"/>

	<node name="commande_locale" pkg="commande_locale" type="simulation" output="screen" launch-prefix="xterm -e" args="$(arg simFile)" />

	<node name="display_node" pkg="commande_locale" type="display_node" output="screen" launch-prefix="xterm -e" />

</launch>
